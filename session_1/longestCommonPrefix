class Solution
    # @param a : array of strings
    # @return a string
    def longestCommonPrefix(a)
        oneString = a.first
        prefix = ""
        
        i = 0
        while i < oneString.size            
            j = 0
            c_prefix = oneString[0..i]            
            while j < a.size    
            p      a[j][0..i]     
                if(a[j][0..i] != c_prefix)                  
                    return a[j][0...i]
                else
                    prefix =  oneString[0..i]
                end
                j += 1
            end
            i += 1
        end

        return prefix

    end
end


p Solution.new.longestCommonPrefix([ "abcd", "abcd", "efgh" ])