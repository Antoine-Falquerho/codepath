Node columnSort(TreeNode treeNode){     Map<Integer, Integer> map = TreeMap<>();          helper(map, treeNode,0);     List<List<Integer>> lists = map.values();        }      void helper(Map<Integer, Integer> map, TreeNode treeNode,int index){     if(treeNode==null){       return;     }     map.put(index,treeNode);     helper(map,treeNode.left,index-1);     helper(map,treeNode.right,index+1);   }